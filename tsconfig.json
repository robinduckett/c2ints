{
  "compilerOptions": {
    // DO NOT EDIT
    "outDir": "./dist",
    "target": "es6",
    "module": "CommonJS",
    "sourceMap": true,
    "strict": true,
    "types": [ "node" ],
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "allowJs": true,
    // Set your preferences here for TypeScript
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": false,
    "allowUnreachableCode": false,
    "noUnusedLocals": true,
    "jsx": "react-jsx",
  },
  "exclude": ["node_modules"],
  "include": ["**/*.ts", "**/*.d.ts", "**/*.tsx"],
}
